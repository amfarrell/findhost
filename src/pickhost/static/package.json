{
  "name": "findhost-assets",
  "version": "0.0.1",
  "description": "Frontend assets for findhost: deciding which friend should host a board game night.",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --config webpack.config.js",
    "watch": "./node_modules/.bin/webpack --config webpack.config.js --watch",
    "tdb": "node-debug ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --colors --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test": "mocha --compilers js:babel-core/register --colors --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "watch-run -p 'assets/js/**/*.@(js|jsx),test/**/*.@(js|jsx)' npm run test",
    "start": "node devserver.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amfarrell/reproduce-webpack-django-tutorial-error.git"
  },
  "author": "Andrew M. Farrell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amfarrell/reproduce-webpack-django-tutorial-error/issues"
  },
  "homepage": "https://github.com/amfarrell/reproduce-webpack-django-tutorial-error#readme",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "geocoder": "^0.2.2",
    "google-geocoding": "^0.1.7",
    "history": "^1.17.0",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "node-debug": "^0.1.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.4.0",
    "react-router": "^1.0.3",
    "redux": "^3.2.1",
    "underscore": "^1.8.3",
    "watch-run": "^1.2.4",
    "webpack": "^1.12.12",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
